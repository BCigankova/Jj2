<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.BorderPane?>


<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="finalproject.client.AppController">
    <top>
        <HBox spacing="10">
            <Region fx:id="spacer" HBox.hgrow="ALWAYS" />
            <Button text="Log In" onAction="#logInAction"/>
            <Button text="Sign Up" onAction="#signUpAction"/>
            <TextField fx:id="username" promptText="Username" />
            <TextField fx:id="password" promptText="Password" />
        </HBox>
    </top>
    <center>

    </center>
</BorderPane>